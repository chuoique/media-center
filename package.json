{
  "name": "media-center",
  "version": "0.1.0",
  "description": "Media center for raspberry pi",
  "main": "lib/index.js",
  "bin": "cli.js",
  "scripts": {
    "start": "npm-run-all --parallel start:frontend start:backend",
    "start:frontend": "webpack-dev-server --hot --inline",
    "start:backend": "npm-run-all --parallel build:backend:watch start:nodemon",
    "start:nodemon": "nodemon --watch lib app.js",
    "build:backend": "babel -s -d lib src",
    "build:backend:watch": "babel --watch -s -d lib src",
    "style": "eslint src/ frontend/",
    "test": "ava ./test/*.js",
    "test:watch": "nodemon test/*.js --exec ava"
  },
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/media-center.git"
  },
  "keywords": [
    "media center",
    "omxplayer"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.10.2",
    "body-parser": "^1.14.2",
    "chokidar": "^1.4.2",
    "cors": "^2.7.1",
    "dot-file-config": "^0.5.0",
    "exit-hook": "^1.1.1",
    "express": "^4.13.3",
    "fkill": "^3.1.0",
    "globby": "^4.0.0",
    "inquirer-credentials": "^1.2.0",
    "inquirer-question": "^1.1.1",
    "keypress": "^0.2.1",
    "meow": "^3.4.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.6.1",
    "node-cache": "^3.1.0",
    "omxplayer": "0.0.1",
    "pouchdb": "^5.1.0",
    "socket.io": "^1.4.2",
    "source-map-support": "^0.4.0",
    "split-torrent-release": "^1.1.0",
    "trakt-utils": "^0.1.1",
    "user-home": "^2.0.0",
    "x11": "^2.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "ava": "^0.9.1",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap": "^3.3.6",
    "chai": "^3.2.0",
