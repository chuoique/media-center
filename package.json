{
  "name": "media-center",
  "version": "0.1.2",
  "description": "Media center for raspberry pi",
  "main": "lib/index.js",
  "bin": "cli.js",
  "scripts": {
    "start": "npm-run-all --parallel start:frontend start:backend",
    "start:frontend": "wserve --x-entry frontend/app.js --x-html frontend/index.html --x-dist public",
    "start:backend": "npm-run-all --parallel build:backend:watch start:nodemon",
    "start:nodemon": "nodemon --watch lib app.js",
    "build:backend": "babel -s -d lib src",
    "build:backend:watch": "babel --watch -s -d lib src",
    "build:frontend": "rm -rf public/* && NODE_ENV=production wbuild --x-entry frontend/app.js --x-html frontend/index.html --x-dist public",
    "lint": "eslint 'src/**/*.js' 'frontend/**/*.js'",
    "test": "ava ./test/*.js",
    "test:watch": "nodemon test/*.js --exec ava"
  },
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/media-center.git"
  },
  "keywords": [
    "media center",
    "omxplayer"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ewnd9/registry": "^1.0.1",
    "bluebird": "^2.10.2",
    "body-parser": "^1.14.2",
    "chokidar": "^1.4.2",
    "cors": "^2.7.1",
    "dot-file-config": "^0.5.0",
    "execa": "^0.4.0",
    "exit-hook": "^1.1.1",
    "express": "^4.13.3",
    "fkill": "^3.1.0",
    "globby": "^4.0.0",
    "got": "^6.2.0",
    "inquirer-credentials": "^1.2.0",
    "inquirer-question": "^1.1.1",
    "keypress": "^0.2.1",
    "meow": "^3.4.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.6.1",
    "node-cache": "^3.1.0",
    "omxplayer": "0.0.1",
    "pouchdb": "^5.1.0",
    "socket.io": "^1.4.2",
    "source-map-support": "^0.4.0",
    "split-torrent-release": "^1.1.0",
    "superagent": "^2.0.0-alpha.2",
    "trakt-utils": "^0.4.0",
    "user-home": "^2.0.0",
    "vlc": "github:fenwick67/node-vlc",
    "x11": "^2.0.2"
  },
  "devDependencies": {
    "alertify.js": "^1.0.8",
    "ava": "^0.14.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap": "^3.3.6",
    "chai": "^3.2.0",
    "classnames": "^2.2.3",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "got": "^6.0.1",
    "husky": "^0.11.3",
    "mocha": "^2.2.5",
    "mock-fs": "^3.6.0",
    "moment": "^2.11.0",
    "nock": "^3.6.0",
    "node-notifier": "^4.4.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.4.0",
    "pre-commit": "^1.1.2",
    "proxyquire": "^1.7.3",
