    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "got": "^6.0.1",
    "isomorphic-fetch": "^2.2.0",
    "mocha": "^2.2.5",
    "mock-fs": "^3.6.0",
    "moment": "^2.11.0",
    "nock": "^3.6.0",
    "node-notifier": "^4.4.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.4.0",
    "postcss-loader": "^0.8.0",
    "pre-commit": "^1.1.2",
